{
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": [
      "^react",
      "^.(css|scss)$",
      "<THIRD_PARTY_MODULES>",
      "^components/(.*)$",
      "^hooks/(.*)$",
      "^[./]"
